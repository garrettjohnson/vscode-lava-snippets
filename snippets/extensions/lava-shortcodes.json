{
  "Shortcode Accordion": {
    "prefix": "scl-accordion",
    "description": "Shortcode Lava",
    "body": [
      "{[ accordion ]}",
      "\t[[ item title:'${2:Small Groups}' ]]", 
      "\t\t$3",
      "\t[[ enditem ]]",
      "\t$4",
      "{[ endaccordion ]}"
    ]
  },
  "Shortcode Accordion Item": {
    "prefix": "scl-accordion-item",
    "description": "Shortcode Lava",
    "body": [
      "[[ item title:'${1:Small Groups}' ]]", 
      "\t$2",
      "[[ enditem ]]"
    ]
  },
  "Shortcode Chart": {
    "prefix": "scl-chart",
    "description": "Shortcode Lava",
    "body": [
      "{[ chart type:'${1|bar,line,radar,pie,doughnut,polarArea|}' ]}",
      "[[ dataitem label:'${2:Small Groups}' value:'${3:45}' ]] [[ enddataitem ]]",
      "\t$2",
      "{[ endgooglemap ]}"
    ]
  },
  "Shortcode Chart Item": {
    "prefix": "scl-chart-item",
    "description": "Shortcode Lava",
    "body": [
      "[[ dataitem label:'${2:Small Groups}' value:'${3:45}' ]] [[ enddataitem ]]"
    ]
  },
  "Shortcode Google Map": {
    "prefix": "scl-map",
    "description": "Shortcode Lava",
    "body": [
      "{[ googlemap ]}",
      "[[ marker location:'${1:10451 W Palmeras Dr Sun City, AZ 85373-2000}' ]] [[ endmarker ]]",
      "\t$2",
      "{[ endgooglemap ]}"
    ]
  },
  "Shortcode Google Map Static": {
    "prefix": "scl-mapstatic",
    "description": "Shortcode Lava",
    "body": [
      "{[ googlestaticmap center:'${1:10451 W Palmeras Dr Sun City, AZ 85373-2000}' zoom:'${2:12}' ]}",
      "{[ endgooglestaticmap ]}"
    ]
  },
  "Shortcode Google Map Static Multi": {
    "prefix": "scl-mapstatic-multi",
    "description": "Shortcode Lava",
    "body": [
      "{[ googlestaticmap ]}",
      "[[ marker location:'${1:1 Cardinals Dr, Glendale, AZ 85305}' ]] [[ endmarker ]]",
      "{[ endgooglestaticmap ]}"
    ]
  },
  "Shortcode Map Marker": {
    "prefix": "scl-mapmarker",
    "description": "Shortcode Lava",
    "body": [
      "[[ marker location:'${1:1 Cardinals Dr, Glendale, AZ 85305}' ]] [[ endmarker ]]"
    ]
  },
  "Shortcode Panel": {
    "prefix": "scl-panel",
    "description": "Shortcode Lava",
    "body": [
      "{[ panel  title:'${1:panel title}' icon:'${2:fa fa-star}' ]}",
      "\t$3",
      "{[ endpanel ]}"
    ]
  },
  "Shortcode Parallax": {
    "prefix": "scl-parallax",
    "description": "Shortcode Lava",
    "body": [
      "{[ parallax image:'${1:https://...}' contentpadding:'${2:20px}' ]}",
      "\t$3",
      "{[ endparallax ]}"
    ]
  },
  "Shortcode Vimeo": {
    "prefix": "scl-vimeo",
    "description": "Shortcode Lava",
    "body": [
      "{[ youtube id:'${1:Id}' ]}"
    ]
  },
  "Shortcode Word Cloud": {
    "prefix": "scl-wordcloud",
    "description": "Shortcode Lava",
    "body": [
      "{[ wordcloud ]}",
      "\t$1",
      "{[ endwordcloud ]}"
    ]
  },
  "Shortcode YouTube": {
    "prefix": "scl-youtube",
    "description": "Shortcode Lava",
    "body": [
      "{[ youtube id:'${1:Id}' controls='${2|true,false|}' autoplay='${3|false,true|}' ]}"
    ]
  }
